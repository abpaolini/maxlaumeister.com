@keyframes bob {
  from {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
  }

  to {
    -webkit-transform: translate(0, 50px);
            transform: translate(0, 50px);
  }
}

@-webkit-keyframes bob {
  from {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
  }

  to {
    -webkit-transform: translate(0, 20px);
            transform: translate(0, 20px);
  }
}

#downArrow {
    display: block;
    margin: 3em auto;
    opacity: 0.5;
    filter: alpha(opacity=50);
    -webkit-transition: all 1s ease;
            transition: all 1s ease;
    -webkit-animation: bob 1s infinite alternate;
    animation: bob 1s infinite alternate;
}

#downArrow, #downArrow svg {
    height: 6em;
    width: 6em;
}

#downArrow.presenting {
    opacity: 0;
    filter: alpha(opacity=0);
}

@media (max-width: 1000px), (max-height: 500px) {
    #extraScroll {
        display: none;
    }
}

@media (min-width: 1000px) and (min-height: 500px) {
    .banner {
        display: none;
    }
}

/* Resume Modal CSS */

#resume-modal {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.85);
    z-index: 2000;
    display: none;
}

#resume-frame {
    width: 100%;
    height: 100%;
    border: none;
}

#resume-container {
    position: absolute;
    top: calc(5vmin + 30px);
    right: 5vmin;
    bottom: 5vmin;
    left: 5vmin;
}

#resume-close {
    color: red;
    text-decoration: none;
    font-size: 45px;
    position: absolute;
    left: 5vmin;
    top: 3vmin;
    line-height: 0.5;
}

.noscroll {
    overflow: hidden;
}

#resume-content, #loading {
    position:absolute; left: 0; right: 0; bottom: 0; top: 0px; 
}

.resumeButton {
    position: absolute;
    padding: 1em;
    text-decoration: none;
    box-shadow: 0.2em 0.2em 0.4em black;
    -webkit-transition: background-color 0.1s ease, opacity 1s;
    transition: background-color 0.1s ease, opacity 1s;
}

#downloadButton {
    right: 2em;
    bottom: 1em;
    line-height: 1.5em;
    border-radius: 1em;
    color: white;
    background-color: #285BB8;
    cursor: default;
    box-sizing: content-box;
    text-decoration: none;
}

#downloadButton.clickable {
    cursor: pointer;
}

#downloadButton.clickable:hover {
